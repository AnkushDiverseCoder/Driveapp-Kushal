{
  "expo": {
    "name": "Kushal Partner",
    "slug": "Driveapp2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/driverLogo.png",
    "scheme": "driveapp2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucifer1902.Driveapp2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "targetSdkVersion": 34,
      "adaptiveIcon": {
        "foregroundImage": "./assets/driverLogo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAPO0jhogONV5axMezWA0rjurIR4NPAnys"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lucifer1902.Driveapp2",
      "gradleProperties": {
        "android.experimental.legacyPageSize": "true",
        "android.useAndroidX": "true",
        "android.enableJetifier": "true"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/driverLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/driverLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Kushal Partner to access your location for work tracking.",
          "locationAlwaysPermission": "Allow Kushal Partner to track your location in the background for continuous work monitoring.",
          "locationWhenInUsePermission": "Allow Kushal Partner to access your location while using the app.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dd2d1037-175b-4be1-aa9d-db52b8d7f86b"
      }
    }
  }
}
